{
  "name": "mcxue-ui",
  "private": false,
  "version": "0.1.0",
  "type": "module",
  "files": [
    "dist/*"
  ],
  "main": "./dist/mcxue-ui.umd.cjs",
  "module": "./dist/mcxue-ui.es.js",
  "exports": {
    ".": {
      "import": "./dist/mcxue-ui.es.js",
      "require": "./dist/mcxue-ui.umd.cjs"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./dist/iconfont.js": {
      "import": "./dist/iconfont.js",
      "require": "./dist/iconfont.js"
    }
  },
  "typings": "./dist/index.d.ts",
  "keywords": [
    "react",
    "typescript",
    "vite",
    "react-ui",
    "mcxue-ui"
  ],
  "author": "mcxue <machaoxue@yeah.net> (https://github.com/mcxue)",
  "license": "MIT",
  "homepage": "https://github.com/mcxue/mcxue-ui",
  "scripts": {
    "dev": "vite",
    "start": "npm run dev",
    "build": "tsc && vite build",
    "build:html": "vite build --config config/build.ts",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/lodash": "^4.14.192",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.30.1",
    "@vitest/ui": "^0.30.1",
    "classnames": "^2.3.2",
    "highlight.js": "^11.7.0",
    "jsdom": "^21.1.1",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0",
    "sass": "^1.58.3",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vitest": "^0.30.1"
  }
}
